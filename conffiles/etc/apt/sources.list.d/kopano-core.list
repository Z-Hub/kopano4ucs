@%@UCRWARNING=# @%@

@!@
if configRegistry.is_true('kopano/repo/kopano-core'):
        username = configRegistry.get('kopano/repo/username', 'serial')
        password = configRegistry.get('kopano/repo/password', 'your-kopano-subscription-key')
        if configRegistry.get('version/version') == "4.2":
                print 'deb https://%s:%s@download.kopano.io/supported/core:/final/Univention_4.2/ /' % (username,password)
                print 'deb https://%s:%s@download.kopano.io/supported/archiver:/final/Univention_4.2/ /' % (username,password)
        elif configRegistry.get('version/version') == "4.3":
                print 'deb https://%s:%s@download.kopano.io/supported/core:/final/Debian_9.0/ /' % (username,password)
                print 'deb https://%s:%s@download.kopano.io/supported/archiver:/final/Debian_9.0/ /' % (username,password)
        else:
                print '# unsupported version'

else:
        print '# This feature is disabled and can be enabled with:'
        print '#   univention-config-registry set kopano/repo/kopano-core=true'
	print '# Read more at: https://wiki.z-hub.io/display/K4U/Updating+Kopano+packages+directly+from+the+Kopano+download+server'
@!@
